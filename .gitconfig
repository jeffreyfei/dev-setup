[alias]
        graph = log --pretty='%Cred%h%Creset | %C(yellow)%d%Creset %s %Cgreen(%cr)%Creset %C(cyan)[%an]%Creset' --graph --all
        cma = commit --all -m
        worddiff = diff --word-diff --unified=10
        stashall = stash save --keep-index --include-untracked
[diff]
	algorithm = histogram
